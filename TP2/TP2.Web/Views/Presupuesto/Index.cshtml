@model IEnumerable<TP2.Entidades.EF.T_GUQ_PRESUPUESTO>

@{
    ViewBag.Title = "Presupuesto";
}

<h2>Presupuesto</h2>

<p>
    @Html.ActionLink("Generar presupuesto", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Año
        </th>
        <th>
            Codigo Presupuesto
        </th>
        <th>
            Area
        </th>
        <th>
            Monto
        </th>
        <th>
           Estado
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.anio)
           
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idPresupuesto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idArea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.idPresupuesto }) |
            @Html.ActionLink("Eliminar", "Details", new { id = item.idPresupuesto })
        </td>
    </tr>
}

</table>
